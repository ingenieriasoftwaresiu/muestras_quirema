}

.ITEMMENU{
    font-family: "Candara";
    font-size: 13px;
    font-weight: normal;
    color: #015637;
    text-align: left;
    background: #FFFFFF;
    padding-left: 10px;
}

.ITEMMENU:hover{
    background: #ECEBA3;
}

a{
    text-decoration: none;
}

a:hover{
    text-decoration: none;
}

a:linked{
    text-decoration: none;
    font-weight: bold;
    font-size: 15px;
}

a:visited{
    text-decoration: none;
}

